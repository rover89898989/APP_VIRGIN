{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/api/*": [
        "src/api/*"
      ],
      "@/shared/*": [
        "src/shared/*"
      ],
      "@/features/*": [
        "src/features/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
