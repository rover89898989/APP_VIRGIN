 {
  "name": "app_virgin",
  "version": "0.1.0",
  "private": true,
  "description": "Monorepo orchestrator for the Expo mobile app (mobile/) and Rust backend (backend/).",
  "license": "MIT",
  "scripts": {
    "mobile:start": "npm --prefix mobile run start",
    "mobile:web": "npm --prefix mobile run web",
    "mobile:android": "npm --prefix mobile run android",
    "mobile:ios": "npm --prefix mobile run ios",
    "mobile:type-check": "npm --prefix mobile run type-check",
    "backend:check": "cargo check --manifest-path backend/Cargo.toml",
    "backend:test": "cargo test --manifest-path backend/Cargo.toml",
    "backend:run": "cargo run --manifest-path backend/Cargo.toml"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  }
 }
